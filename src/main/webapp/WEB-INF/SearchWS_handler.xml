    <handler>
      <handler-name>com.mycompany.flights.handlers.NewMessageHandler</handler-name>
      <handler-class>com.mycompany.flights.handlers.NewMessageHandler</handler-class>
    </handler>
  </handler-chain>
</handler-chains>
